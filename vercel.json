{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "builds": [
    { "src": "index.js", "use": "@vercel/node@4.0.0" },
    { "src": "api/*.js", "use": "@vercel/node@4.0.0" }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/(.*)", "dest": "index.js" }
  ]
}
