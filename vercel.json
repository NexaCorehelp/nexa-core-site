{
	"version": 2,
	"cleanUrls": true,
	"trailingSlash": false,
	"builds": [
		{ "src": "index.js", "use": "@vercel/node@4.0.0" },
		{ "src": "api/*.js", "use": "@vercel/node@4.0.0" }
	],
	"routes": [
		{ "src": "/nexion/(.*)", "dest": "index.js" },
		{ "src": "/api/(.*)", "dest": "/api/$1" },
		{ "src": "/(.*)", "dest": "index.js" }
	]
}
